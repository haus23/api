{
    "name": "haus23/api",
    "description": "Web API for https://haus23.net",
    "version": "0.1.1",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Micha Alt",
            "email": "micha@haus23.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require": {
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/yaml": "^4.2",
        "symfony/twig-bundle": "^4.2",
        "doctrine/annotations": "^1.6",
        "symfony/console": "^4.2",
        "symfony/monolog-bundle": "^3.3",
        "symfony/messenger": "^4.2"
    },
    "require-dev": {
        "symfony/debug-pack": "^1.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    }
}
